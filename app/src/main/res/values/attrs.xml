<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SettingsItemView">
        <attr name="title" format="string" />
        <attr name="description" format="string" />
    </declare-styleable>

    <declare-styleable name="ToggleSettingItemView">
        <attr name="checked" format="boolean" />
    </declare-styleable>

    <declare-styleable name="PagerIndicator">
        <attr name="indicatorRadius" format="dimension" />
        <attr name="indicatorMargin" format="dimension" />
        <attr name="indicatorEmptyColor" format="color" />
        <attr name="indicatorFillColor" format="color" />
    </declare-styleable>
</resources>
